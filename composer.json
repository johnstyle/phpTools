            "src/sJo/Helpers/Autoload.php",
            "src/sJo/Helpers/DateInterval.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    },
    "minimum-stability": "dev"
