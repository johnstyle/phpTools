{
    "name": "johnstyle/sjo",
    "type": "library",
    "description": "PHP 5 MVC Framework",
    "keywords": ["PHP 5", "PHP5", "MVC", "Framework", "sJo", "PHPTools"],
    "homepage": "https://github.com/johnstyle/sjo",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Jonathan Sahm",
            "email": "contact@johnstyle.fr",
            "homepage": "http://www.johnstyle.fr",
            "role": "Developer"
        }
    ],
    "support": {
